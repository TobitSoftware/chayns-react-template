{
    "name": "chayns-react-template",
    "version": "1.0.0",
    "private": true,
    "keywords": [
        "chayns",
        "chayns-toolkit",
        "react"
    ],
    "license": "MIT",
    "scripts": {
        "build": "chayns-toolkit build",
        "dev": "chayns-toolkit dev",
