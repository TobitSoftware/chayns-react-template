    },
    "dependencies": {
        "chayns-components": "^4.19.1",
        "prop-types": "^15.7.2",
        "react": "^16.13.1",
        "react-dom": "^16.13.1"
    },
    "devDependencies": {
        "chayns-toolkit": "^1.3.0",
        "prettier": "^2.1.2",
        "prettier-plugin-packagejson": "^2.2.6"
    }
}
